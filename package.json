{
  "name": "you54fdotcom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.tsx,.ts ./src",
    "format": "prettier-eslint --config package.json --write src/**/*.{ts,tsx,js}",
    "build:css": "tailwind build src/ubuntu/styles/styles.css -o src/ubuntu/styles/main.css",
    "predeploy": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix --ext .js ./src"
    ],
    "*.{tsx}": [
      "eslint --fix --ext .tsx ./src"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5"
        }
      ]
    },
    "ignorePatterns": [
      "node_modules/",
      "build/"
    ]
  },
  "dependencies": {
    "@rooks/use-raf": "^4.11.2",
    "date-fns": "2.21.3",
    "emailjs-com": "^2.6.4",
    "framer-motion": "^4.1.16",
    "jquery": "^3.6.0",
    "nightwind": "^1.1.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.3",
    "react-ga": "^3.3.0",
    "react-helmet-async": "^1.0.4",
    "react-icons": "^4.2.0",
    "react-markdown": "^6.0.2",
    "react-onclickoutside": "^6.10.0",
    "react-rangeslider": "2.2.0",
    "react-redux": "^7.2.4",
    "react-rnd": "^10.2.4",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-syntax-highlighter": "^15.4.3",
    "react-terminal": "^1.1.0",
    "react-webcam": "5.2.3",
    "recompose": "^0.26.0",
    "redux": "^4.1.0",
    "remark-gfm": "^1.0.0",
    "styled-components": "^5.0.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@craco/craco": "^6.1.2",
    "@teamsupercell/typings-for-css-modules-loader": "^2.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/eslint": "^6.1.1",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/prettier": "^1.18.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-onclickoutside": "^6.7.3",
    "@types/react-redux": "^5.0.21",
    "@types/react-rnd": "^7.4.4",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/styled-components": "^4.4.2",
    "autoprefixer": "^9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "postcss": "^7.0.35",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.1",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.1",
    "sort-package-json": "^1.50.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-node": "^8.6.2",
    "typescript": "4.2.4"
  }
}
